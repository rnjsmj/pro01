package org.gh.dto;

public class Traffic {
	private int tno; 	//연번
	private String ttype; //종류
	private String no;		  //번호/노선
	private String route; //노선도
	private String comm;  //설명
	
	
	public Traffic(int tno, String ttype, String no, String route, String comm) {
		super();
		this.tno = tno;
		this.ttype = ttype;
		this.no = no;
		this.route = route;
		this.comm = comm;
	}
	
	public Traffic() {}
	
	

	public int getTno() {
		return tno;
	}

	public void setTno(int tno) {
		this.tno = tno;
	}

	public String getTtype() {
		return ttype;
	}

	public void setTtype(String ttype) {
		this.ttype = ttype;
	}

	public String getNo() {
		return no;
	}

	public void setNo(String no) {
		this.no = no;
	}

	public String getRoute() {
		return route;
	}

	public void setRoute(String route) {
		this.route = route;
	}

	public String getComm() {
		return comm;
	}

	public void setComm(String comm) {
		this.comm = comm;
	}

	@Override
	public String toString() {
		return "Data [ttype=" + ttype + ", no=" + no + ", route=" + route + ", comm=" + comm + "]";
	}
	
	
}
